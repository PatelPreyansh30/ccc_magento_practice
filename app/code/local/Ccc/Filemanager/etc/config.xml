<?xml version="1.0"?>
<config>
    <modules>
        <Ccc_Filemanager>
            <version>1.6.0.0</version>
        </Ccc_Filemanager>
    </modules>
    <adminhtml>
        <layout>
            <updates>
                <ccc_filemanager>
                    <file>ccc_filemanager.xml</file>
                </ccc_filemanager>
            </updates>
        </layout>
    </adminhtml>
    <global>
        <blocks>
            <ccc_filemanager>
                <class>Ccc_Filemanager_Block</class>
            </ccc_filemanager>
        </blocks>
        <helpers>
            <ccc_filemanager>
                <class>Ccc_Filemanager_Helper</class>
            </ccc_filemanager>
        </helpers>
        <models>
            <ccc_filemanager>
                <class>Ccc_Filemanager_Model</class>
            </ccc_filemanager>
        </models>
    </global>
    <admin>
        <routers>
            <adminhtml>
                <args>
                    <modules>
                        <ccc_filemanager_admin>Ccc_Filemanager_Adminhtml</ccc_filemanager_admin>
                    </modules>
                </args>
            </adminhtml>
        </routers>
    </admin>
</config>